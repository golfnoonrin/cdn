function setAppByUser(){
  const userLevel = document.getElementById('user_level_status').innerHTML
  // console.log(userLevel)
  if ( userLevel == 'admin_a' || userLevel == 'admin_p'){
    document.getElementById("admin_menu_li").classList.remove("d-none")
    document.getElementById("sidebar_setting_li").classList.remove("d-none")
    document.getElementById("setting_view_oc_li").classList.remove("d-none")
    document.getElementById("user_level").innerHTML = 'Admin'

    
  } else {
    document.getElementById("admin_menu_li").classList.add("d-none")
    document.getElementById("sidebar_setting_li").classList.add("d-none")
    document.getElementById("setting_view_oc_li").classList.add("d-none")
    document.getElementById("user_level").innerHTML = 'User'
  }

};
